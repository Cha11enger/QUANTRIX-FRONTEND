meta {
  name: GetToken
  type: http
  seq: 1
}

get {
  url: https://httpbin.org/get
  body: none
  auth: none
}

script:pre-request {
  const token = bru.getEnvVar('actualuser.accessToken');
  console.log('Current token:', token);
  console.log('Token exists:', token ? 'YES' : 'NO');
  if (token) {
    console.log('Token first 20 chars:', token.substring(0, 20) + '...');
  }
}